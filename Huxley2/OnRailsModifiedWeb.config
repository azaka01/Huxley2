<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\Huxley2.dll" stdoutLogEnabled="false" stdoutLogFile="\\?\%home%\LogFiles\stdout" hostingModel="inprocess" />
       <security>
           <requestFiltering>
                 <hiddenSegments>
                    <remove segment="BIN" />
                </hiddenSegments>
            </requestFiltering>
        </security>
    </system.webServer>
  </location>
</configuration>